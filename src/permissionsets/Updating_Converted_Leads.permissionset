<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Standard profiles cannot update Converted Leads. Use this permission set if you need to use the data loader to update Converted Leads. I have added the System Permission - Enable &quot;Set Audit Fields upon Record Creation&quot;</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Updating Converted Leads</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>CreateAuditFields</name>
    </userPermissions>
</PermissionSet>
